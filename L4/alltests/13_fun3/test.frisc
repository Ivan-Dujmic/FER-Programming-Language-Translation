	MOVE 40000, R7
	CALL F_main
	HALT

F_f
	SUB R7, %D 4, R7
	LOAD R1, (R7+08)
	STORE R1, (R7+00)
	LOAD R1, (R7+00)
	PUSH R1
	MOVE %D 5, R1
	PUSH R1
	POP R2
	POP R1
	ADD R1, R2, R1
	PUSH R1
	POP R6
	ADD R7, %D 4, R7
	RET

F_main
	MOVE %D 7, R1
	PUSH R1
	CALL F_f
	ADD R7, %D 4, R7
	PUSH R6
	MOVE %D -4, R1
	PUSH R1
	CALL F_f
	ADD R7, %D 4, R7
	PUSH R6
	POP R2
	POP R1
	ADD R1, R2, R1
	PUSH R1
	POP R6
	ADD R7, %D 0, R7
	RET
